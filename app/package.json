{
    "name": "cl-dashboard",
    "version": "1.0.0",
    "description": "cl-dashboard admin template",
    "main": "src/index.js",
    "scripts": {
        "start": "run-p start:dev",
        "start:dev": "webpack serve --config webpack.config.dev.js --port 5000",
        "test": "jest --watch",
        "clean:build": "rimraf ./build && mkdir build",
        "prebuild": "run-p clean:build ",
        "build-staging": "webpack serve --config webpack.config.staging.js",
        "build-prod": "webpack serve --config webpack.config.prod.js"
    },
    "keywords": [
        "reactjs"
    ],
    "author": "hafis alrizal",
    "license": "MIT",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-regular-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "bootstrap": "^5.0.2",
        "classnames": "^2.3.1",
        "file-loader": "^6.2.0",
        "file-saver": "^2.0.5",
        "formik": "^2.2.9",
        "node-sass": "^6.0.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-app-polyfill": "^2.0.0",
        "react-base-table": "^1.13.0",
        "react-bootstrap": "^2.0.0-beta.4",
        "react-dom": "^17.0.2",
        "react-pro-sidebar": "^0.6.0",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-select": "^5.2.2",
        "react-toastify": "^7.0.4",
        "recharts": "^2.0.10",
        "redux": "^4.1.0",
        "sass-loader": "^12.1.0",
        "styled-components": "^5.3.0",
        "universal-cookie": "^4.0.4",
        "url-loader": "^4.1.1",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-preset-react-app": "^10.0.0",
        "css-loader": "^6.2.0",
        "cssnano": "^5.0.7",
        "eslint": "^7.31.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-react": "^7.24.0",
        "html-webpack-plugin": "^5.3.2",
        "jest": "^27.0.6",
        "mini-css-extract-plugin": "^2.1.0",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^6.1.1",
        "rimraf": "^3.0.2",
        "style-loader": "^3.2.1",
        "webpack": "^5.46.0",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    },
    "jest": {
        "setupFiles": [
            "./tools/testSetup.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/fileMock.js",
            "\\.(css|less)$": "<rootDir>/tools/styleMock.js"
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=10",
        "npm": ">=6"
    },
    "babel": {
        "presets": [
            "babel-preset-react-app"
        ]
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "plugin:import/errors",
            "plugin:import/warnings"
        ],
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 2018,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "env": {
            "browser": true,
            "node": true,
            "es6": true,
            "jest": true
        },
        "rules": {
            "no-debugger": "off",
            "no-console": "off",
            "no-unused-vars": "warn",
            "react/prop-types": "warn"
        },
        "settings": {
            "react": {
                "version": "detect"
            }
        },
        "root": true
    }
}
