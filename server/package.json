{
  "name": "mern-stack-boilerplate-typescript",
  "version": "1.0.0",
  "description": "MERN Stack Boilerplate Typescript",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cforclown/mern-stack-boilerplate-typescript"
  },
  "scripts": {
    "init": "npm run init:dev",
    "start": "ts-node src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "init:dev": "set NODE_ENV=development&& ts-node src/scripts/insert-initial-data.ts",
    "init:prod": "set NODE_ENV=production&& ts-node src/scripts/insert-initial-data.ts",
    "dev": "set NODE_ENV=development&& ts-node src/index.ts",
    "prod": "set NODE_ENV=production&& ts-node src/index.ts",
    "lint": "eslint \"src/**/*.{js,ts}\" \"__mock__/**/*.{js,ts}\"",
    "lint:check": "npm run lint",
    "lint:format": "npm run lint -- --fix",
    "lint:staged": "lint-staged"
  },
  "author": "hafis alrizal",
  "license": "ISC",
  "dependencies": {
    "awilix": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^6.1.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.30",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.2.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.3",
    "@types/jest-when": "^3.5.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/memoizee": "^0.4.6",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.11.12",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "jest-mock-req-res": "^1.0.2",
    "jest-when": "^3.5.1",
    "license-checker": "^25.0.1",
    "mockgoose": "^8.0.4",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint-check"
    ]
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.4.0"
  }
}
